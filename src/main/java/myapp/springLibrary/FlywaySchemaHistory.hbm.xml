<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="myapp.springLibrary.FlywaySchemaHistory" table="flyway_schema_history" schema="e_learning">
        <id name="installedRank" column="installed_rank"/>
        <property name="version" column="version"/>
        <property name="description" column="description"/>
        <property name="type" column="type"/>
        <property name="script" column="script"/>
        <property name="checksum" column="checksum"/>
        <property name="installedBy" column="installed_by"/>
        <property name="installedOn" column="installed_on"/>
        <property name="executionTime" column="execution_time"/>
        <property name="success" column="success"/>
    </class>
</hibernate-mapping>